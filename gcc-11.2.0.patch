diff -Naur gcc-11.2.0/gcc/config/i386/onyx64.h gcc-11.2.0-patched/gcc/config/i386/onyx64.h
--- gcc-11.2.0/gcc/config/i386/onyx64.h	1970-01-01 01:00:00.000000000 +0100
+++ gcc-11.2.0-patched/gcc/config/i386/onyx64.h	2021-12-01 04:35:01.065713610 +0000
@@ -0,0 +1,35 @@
+/* SSP and Split stack support taken verbatim from gnu-user64.h
+ */
+#undef TARGET_THREAD_SSP_OFFSET
+#undef TARGET_THREAD_SPLIT_STACK_OFFSET
+
+
+#define TARGET_THREAD_SSP_OFFSET \
+  (TARGET_64BIT ? (TARGET_X32 ? 0x18 : 0x28) : 0x14)
+
+/* i386 glibc/musl provides __private_ss in %gs:0x30.
+   x32 glibc/musl provides it in %fs:0x40.
+   x86_64 glibc/musl provides it in %fs:0x70.  */
+#define TARGET_THREAD_SPLIT_STACK_OFFSET \
+  (TARGET_64BIT ? (TARGET_X32 ? 0x40 : 0x70) : 0x30)
+
+#if TARGET_64BIT_DEFAULT
+#define SPEC_32 "m16|m32"
+#if TARGET_BI_ARCH == 2
+#define SPEC_64 "m64"
+#define SPEC_X32 "m16|m32|m64:;"
+#else
+#define SPEC_64 "m16|m32|mx32:;"
+#define SPEC_X32 "mx32"
+#endif
+#else
+#define SPEC_32 "m64|mx32:;"
+#define SPEC_64 "m64"
+#define SPEC_X32 "mx32"
+#endif
+
+#define ONYX_ARCH_SUFFIX  "%{" SPEC_X32 ":x32}%{" SPEC_32 ":i386}%{" SPEC_64 ":x86_64}"
+
+#undef LD_ONYX_PATH
+
+#define LD_ONYX_PATH  "/lib/ld-onyx-" ONYX_ARCH_SUFFIX ".so"
diff -Naur gcc-11.2.0/gcc/config/i386/t-onyx gcc-11.2.0-patched/gcc/config/i386/t-onyx
