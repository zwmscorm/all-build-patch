+PKGNAME=gcc
+PKGVER=11.2.0
+
+build() {
+	cd ..
+	mkdir -p gcc-build
+	cd gcc-build
+	export CC_FOR_TARGET=x86_64-onyx-gcc
+	export CXX_FOR_TARGET=x86_64-onyx-g++
+	export LD_FOR_TARGET=x86_64-onyx-ld
+	../${PKGNAME}-${PKGVER}/configure --host=x86_64-onyx --prefix=/usr --with-sysroot= --with-build-sysroot=/home/pfalcato/Onyx/sysroot --enable-languages=c,c++ --disable-nls --enable-threads=posix --enable-libstdcxx-threads --enable-symvers=gnu --enable-default-pie --enable-lto --enable-default-ssp --enable-shared
+	$MAKE all-gcc all-target-libgcc all-target-libstdc++-v3
+}
+
+package() {
+	cd ../gcc-build
+	DESTDIR=$PKGDIR $MAKE install-gcc install-target-libgcc install-target-libstdc++-v3
